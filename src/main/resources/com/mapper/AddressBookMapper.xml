<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.AddressBookMapper">

    <resultMap id="BaseResultMap" type="com.pojo.AddressBook">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="contactPerson" column="contact_person" jdbcType="VARCHAR"/>
            <result property="phone" column="phone" jdbcType="CHAR"/>
            <result property="city" column="city" jdbcType="VARCHAR"/>
            <result property="workplace" column="workplace" jdbcType="VARCHAR"/>
            <result property="note" column=" note" jdbcType="VARCHAR"/>
            <result property="userId" column="user_id" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,contact_person,phone,
        city,workplace, note,
        user_id
    </sql>
</mapper>
